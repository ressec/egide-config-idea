          <IdeaActionID>RunClass</IdeaActionID>
          <ShortCut>'⌃⇧R'</ShortCut>
          <Description>Run 'Tests in 'mobile.Glu...'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Diff">
        <Item>
          <IdeaActionID>Diff.ShowDiff</IdeaActionID>
          <ShortCut>'⌘D'</ShortCut>
          <Description>Show Diff</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Demo'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'⌘F2'</ShortCut>
          <Description>Stop 'Demo'</Description>
